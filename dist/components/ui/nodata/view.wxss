.c-nodata-wrap {
  text-align: center;
  display: flex;
  height: 100%;
  align-items: center;
  justify-content: center;
  min-height: 445rpx;
  background: #fff;
}
.c-nodata-wrap .c-img {
  height: 220rpx;
  width: 220rpx;
  display: inline-block;
}
.c-nodata-wrap .c-img image {
  width: 100%;
  height: 100%;
}
.c-nodata-wrap .c-title {
  margin-top: 34rpx;
  font-size: 32rpx;
  color: #333;
}
