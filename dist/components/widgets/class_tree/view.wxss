.w-class-tree {
  width: 160rpx;
  border-right: 1rpx solid #efefef;
  height: 100%;
  background: #fff;
}
.w-class-tree-main {
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
  padding-left: 20rpx;
  position: relative;
}
.w-class-tree-main.w-class-tree-main-active {
  color: #00994f;
}
.w-class-tree-main.w-class-tree-main-active label {
  border-left: 10rpx solid #00994f;
  position: absolute;
  left: 0;
  top: 28rpx;
  height: 32rpx;
}
.w-class-tree-item {
  height: 72rpx;
  line-height: 72rpx;
  padding-left: 44rpx;
  color: #666;
  font-size: 24rpx;
  position: relative;
}
.w-class-tree-item-flag {
  border-radius: 50%;
  width: 10rpx;
  height: 10rpx;
  background: #bfbfbf;
  position: absolute;
  top: 30rpx;
  left: 22rpx;
}
.w-class-tree-item-active {
  color: #00994f;
}
.w-class-tree-item-active-flag {
  background: #00994f;
}
/* ui组件样式覆盖 */
.w-collapse-item {
  border-bottom: 1rpx solid #efefef;
}
.w-collapse-item .w-collapse-item-title {
  border: 0 !important;
  padding: 0 !important;
}
