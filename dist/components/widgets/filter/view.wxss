.w-filter-wrap .w-filter {
  position: fixed;
  z-index: 101;
  top: 0;
  left: 0;
  right: 0;
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 1rpx solid #efefef;
  background: #fff;
}
.w-filter-wrap .w-filter-names {
  flex: 1;
  text-align: center;
  color: #666;
}
.w-filter-wrap .w-filter-names-active {
  color: #00994f;
}
.w-filter-wrap .w-filter-names text,
.w-filter-wrap .w-filter-names c-icon {
  display: inline-block;
  vertical-align: middle;
}
.w-filter-wrap .w-filter-names c-icon {
  vertical-align: top;
  margin-left: 10rpx;
  color: #999;
}
.w-filter-wrap .w-filter-names label {
  font-size: 14rpx;
}
.w-filter-wrap .w-filter-group {
  margin-top: 88rpx;
}
.w-filter-wrap .w-operate {
  display: flex;
}
.w-filter-wrap .w-operate c-button {
  flex: 1;
}
